{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "data": {"url": "https://raw.githubusercontent.com/Alucardigan/Data_Viz_A2/refs/heads/main/data/incomeUsedByHousehold.csv"},
    "width": "container",
    "height": "container",
    "mark": "bar",
    "encoding": {
      "x": {
        "timeUnit": "year",
        "field": "Year",
        "type": "ordinal",
        "title": "Year"
      },
      "y": {
        "field":"Percentage",
        "type": "quantitative",
        "stack":true
      },
      "color": {
        "field": "Quantile",
        "type": "nominal",
        "title": "Quantile"
      }
    }
  }
  