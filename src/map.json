{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "description": "A map of Australia showing rental prices by state.",
    "width": 600,
    "height": 400,
    "data": {
      "url": "data/AustralianMapWithBorders.json",  
      "format": {
        "type": "json"
      }
