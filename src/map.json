{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
  "description": "A map of Australia showing rental prices by state.",
  "width": 600,
  "height": 400,
  "data": {
    "url": "data/AustralianMapWithBorders.json",  
    "format": {
      "type": "topojson",
      "feature": "ne_50m_admin_1_states_provinces"
    }
  },
  "projection": {
    "type": "mercator"  
  },
  "mark": {
    "type": "geoshape",
    "stroke": "black",
    "strokeWidth": 0.5
  }
}