{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
  "description": "A map of Australia showing rental prices by state.",
  "width": 600,
  "height": 400,
  "data": {
    "url": "data/AustralianMapWithBorders.json",
    "format": {
      "type": "topojson",
      "feature": "ne_50m_admin_1_states_provinces"
    }
  },
  "transform": [
    {
      "lookup": "properties.name",
      "from": {
        "data": {
          "url": "data/MedianHousePricesByState.csv"
        },
        "key": "State",
        "fields": ["Median Housing Price", "Date"]
      }
    },
    {
      "calculate": "toDate(datum['Date'])",
      "as": "ParsedDate"
    }
  ],
  "params": [
    {
      "name": "dateRange",
      "bind": {
        "input": "range",
        "min": 1017619200000,  
        "max": 1025568000000,  
        "step": 7776000000,    
        "name": "Date Range: "
      }
    }
  ],
  "projection": {
    "type": "mercator"
  },
  "mark": {
    "type": "geoshape",
    "stroke": "black",
    "strokeWidth": 0.5
  },
  "encoding": {
    "color": {
      "field": "Median Housing Price",
      "type": "quantitative"
    },
    "tooltip": [
      {"field": "properties.name", "type": "nominal", "title": "State"},
      {"field": "Median Housing Price", "type": "quantitative", "title": "Median Price"},
      {"field": "ParsedDate", "type": "temporal", "title": "Date"}
    ]
  }
}
